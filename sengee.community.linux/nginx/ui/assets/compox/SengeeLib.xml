	<compox id="sengee-bar" type="CHART" series="ECHARTS">
		<configuration>
			<dataConfig>
        <parameter name="x" c_Alias="横轴" c_Compox="sightx-combox"></parameter>
        <parameter name="y" c_Alias="纵轴" c_Compox="sightx-combox"></parameter>
			</dataConfig>
			<styleConfig>
        <parametergroup c_Alias="柱体样式" c_Compox="sengee-group">
          <parameter name="series.showBackground" c_Alias="是否显示柱条的背景色" c_Compox="sightx-switch"></parameter>
          <parameter name="series.backgroundStyle.color" c_Alias="每一个柱条的背景颜色" c_Compox="sengee-color"></parameter>
          <parameter name="series.barWidth" c_Alias="柱条的宽度" c_Compox="sengee-percentage" c_Tooltip="默认自适应宽度。0% 时自适应宽度。"></parameter>
        </parametergroup>
        <parametergroup c_Alias="横轴样式" c_Compox="sengee-group">
          <parameter name="xAxis.show" c_Alias="是否显示横轴" c_Compox="sightx-switch"></parameter>
          <parameter name="xAxis.axisLabel.rotate" c_Alias="横轴文本的旋转角度" c_Compox="sengee-number"></parameter>
        </parametergroup>
        <parametergroup c_Alias="边距" c_Compox="sengee-group">
          <parameter name="grid.top" c_Alias="上边距" c_Compox="sengee-percentage"></parameter>
          <parameter name="grid.bottom" c_Alias="下边距" c_Compox="sengee-percentage"></parameter>
          <parameter name="grid.left" c_Alias="左边距" c_Compox="sengee-percentage"></parameter>
          <parameter name="grid.right" c_Alias="右边距" c_Compox="sengee-percentage"></parameter>
        </parametergroup>
			</styleConfig>
		</configuration>
